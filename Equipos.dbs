			<index name="pk_ip" unique="PRIMARY_KEY" >
				<column name="Id_Ip" />
			</index>
		</table>
		<table name="Inventario_Equipos" prior="Equipos" >
			<comment><![CDATA[Características 
 Catálogos
 Estatus
Servicios
 Tipos Servicios
Resguardantes
Área asignación
 Ip]]></comment>
			<column name="Id" type="INT" jt="4" mandatory="y" />
			<column name="caracteristicas" type="VARCHAR" length="255" jt="12" />
			<column name="catalogos" type="INT" jt="4" />
			<column name="estatus" type="CHAR" length="1" jt="1" />
			<column name="resguardantes" type="INT" jt="4" />
			<column name="area_asignada" type="INT" jt="4" />
			<column name="IP" type="VARCHAR" length="255" jt="12" />
			<index name="pk_equipos" unique="PRIMARY_KEY" >
				<column name="Id" />
			</index>
		</table>
		<table name="Permisos_Eq" prior="entity" />
		<table name="Permisos_Equipo" prior="entity" >
			<column name="Id_Permisos_Equipo" prior="Id" type="INT" jt="4" mandatory="y" />
			<column name="Fechas_Asignacion" type="DATE" jt="91" />
			<column name="Solicitud_Id_Tipo_Solicitud" prior="Id_Tipo_Solicitud" type="CHAR" length="8" jt="1" />
			<index name="pk_permisos_equipo" unique="PRIMARY_KEY" >
				<column name="Id_Permisos_Equipo" />
			</index>
			<fk name="fk_permisos_equipo_control_ips" virtual="y" to_schema="equipos.INSTANCE" to_table="Control_Ips" type="OneToOne" >
				<fk_column name="Id_Permisos_Equipo" pk="Id_Permisos_Equipo" />
			</fk>
		</table>
		<table name="Tipo_Solicitud" prior="entity" >
			<comment><![CDATA[Oficio
Verbal
Instrucción
Tiempo]]></comment>
			<column name="Id_Tipo_Solicitud" prior="Id_Tip" type="INT" jt="4" mandatory="y" />
			<column name="Medio" type="VARCHAR" length="25" jt="12" />
			<column name="Tiempo" type="VARCHAR" length="25" jt="12" />
			<index name="pk_tipo_solicitud" unique="PRIMARY_KEY" >
				<column name="Id_Tipo_Solicitud" />
			</index>
			<fk name="fk_tipo_solicitud_permisos_equipo" virtual="y" to_schema="equipos.INSTANCE" to_table="Permisos_Equipo" type="OneToOne" >
				<fk_column name="Id_Tipo_Solicitud" pk="Solicitud_Id_Tipo_Solicitud" />
			</fk>
		</table>
		<table name="calendarización" prior="entity" >
			<comment><![CDATA[Calendarización por área
Calendarización por Equipos
Alertas]]></comment>
			<column name="Id" type="INT" jt="4" mandatory="y" />
