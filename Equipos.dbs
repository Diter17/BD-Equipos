			<column name="Usuario" type="BOOLEAN" jt="-7" >
				<defo><![CDATA[null]]></defo>
			</column>
			<column name="Tipo_Equipo" prior="Id_Tipo_Equipo" type="VARCHAR" length="25" jt="12" />
			<column name="Id_Permisos_Equipo" type="CHAR" length="8" jt="1" />
			<index name="pk_control_ips" unique="PRIMARY_KEY" >
				<column name="Id_Control_Ip" />
			</index>
			<fk name="fk_control_ips_ip" virtual="y" to_schema="equipos.INSTANCE" to_table="IP" type="OneToOne" >
				<fk_column name="Id_Control_Ip" pk="Id_Control_Ip" />
			</fk>
		</table>
		<table name="ID" prior="entity" >
			<comment><![CDATA[Personales
Laborales]]></comment>
			<column name="Tipo de dispositivo" type="INT" jt="4" />
		</table>
		<table name="IP" prior="entity" >
			<column name="Id_Ip" type="INT" jt="4" mandatory="y" />
			<column name="IP" type="VARCHAR" length="15" jt="12" />
